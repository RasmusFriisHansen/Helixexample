<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <sites>
      <site name="Example" patch:after="site[@name='modules_website']"
            database="web"
            virtualFolder="/"
            physicalFolder="/"
            rootPath="/sitecore/content/All Sites/Main Site/Example"
            startItem="/Home"
            cacheHtml="true"
            htmlCacheSize="50MB"
            registryCacheSize="0"
            viewStateCacheSize="0"
            xslCacheSize="25MB"
            filteredItemsCacheSize="10MB"
            enablePreview="true"
            enableWebEdit="true"
            enableDebugger="true"/>
    </sites>
  </sitecore>
</configuration>
